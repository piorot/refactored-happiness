  "dependencies": {},
  "devDependencies": {
    "@angular/cli": "^1.4.7",
    "bower": "^1.8.2",
    "http-server": "^0.10.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.8",
    "karma-junit-reporter": "^0.4.1",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^1.3.2",
