{
  "name": "refactored-happiness",
  "version": "1.0.0",
  "description": "Happiness refactoring in progress",
  "main": "app.js",
  "scripts": {
    "postinstall": "bower install",
    "update-deps": "npm update",
    "postupdate-deps": "bower update",
    "start": "http-server -a 0.0.0.0 -c-1 ./client",
    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js",
    "coveralls": "cat ./client/coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piorot/refactored-happiness.git"
  },
  "keywords": [
    "angular"
  ],
  "author": "piorot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/piorot/refactored-happiness/issues"
  },
  "homepage": "https://github.com/piorot/refactored-happiness#readme",
  "dependencies": {
    "bower": "^1.8.2",
    "http-server": "^0.10.0"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.7",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "http-server": "^0.10.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-coverage": "^1.1.1",
    "karma-coveralls": "^1.1.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.8",
    "karma-junit-reporter": "^0.4.1",
    "karma-mocha": "^1.3.0",
    "karma-ng-html2js-preprocessor": "^1.0.0",
    "karma-sinon-chai": "^1.3.2",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "protractor": "^4.0.9",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.14.0",
    "standard": "^10.0.3"
  }
}
